        al_draw_text(font2,al_map_rgb(255,255,255),2380,400,ALLEGRO_ALIGN_LEFT,"por que nao acabou com aqueles");
        al_draw_text(font2,al_map_rgb(255,255,255),2380,415,ALLEGRO_ALIGN_LEFT,"olhos sozinho?.");
    }
      if (fala3_3)
    {
