    al_set_sample_instance_playmode(SONGBOSS2R, ALLEGRO_PLAYMODE_LOOP);
    al_set_sample_instance_playmode(SONGBOSS2F, ALLEGRO_PLAYMODE_ONCE);
    al_set_sample_instance_playmode(SONGBOAT, ALLEGRO_PLAYMODE_LOOP);
