    al_destroy_bitmap(fundorank);
    al_destroy_bitmap(fundoranking);
    al_destroy_bitmap(BRIDGE);
    al_destroy_bitmap(CRISTAL1);
    al_destroy_bitmap(CRISTAL2);
    al_destroy_bitmap(CRISTAL3);
    al_destroy_bitmap(PLATACAVE7);
    al_destroy_bitmap(ESTALAC);
    al_destroy_bitmap(BLACKANDA);
    al_destroy_bitmap(BLACKVOA);
    al_destroy_bitmap(BLACKESP1);
    al_destroy_bitmap(BLACKESP2);
    al_destroy_bitmap(BLACKESP22);
    al_destroy_bitmap(BLACKESP3);
    al_destroy_bitmap(BLACKESP32);
    al_destroy_sample(MUSICAOP);
    al_destroy_sample(MUSICAFASE);
    al_destroy_sample(MUSICABOSS1);
    al_destroy_sample(MUSICABOSS2);
    al_destroy_sample(MUSICABOSS2R);
    al_destroy_sample(MUSICABOSS2F);
    al_destroy_sample(MUSICABOAT);
    al_destroy_sample_instance(SONGINSTANCE);
    al_destroy_sample_instance(SONGFASE);
    al_destroy_sample_instance(SONGBOSS1);
    al_destroy_sample_instance(SONGBOSS2);
    al_destroy_sample_instance(SONGBOSS2R);
    al_destroy_sample_instance(SONGBOSS2F);
    al_destroy_sample_instance(SONGBOAT);
    al_destroy_font(font5);
    al_destroy_event_queue(fila_eventos);


    fstream myfile ("Nomes.txt", ios::in | ios::out | ios::ate);

    if (myfile.is_open())
    {

        cout << "Ola " << str << ". Voce pode acessar o arquivo.txt na pasta \ndo jogo para ver o nome de quem tambem jogou";

        myfile << str  << " - Score: " << score << "\n";
        myfile.close();
    }
    else
        cout << "Falha ao criar lista de nomes!";


    return 0;
}

void manipular_entrada(ALLEGRO_EVENT evento)
{

    if (evento.type == ALLEGRO_EVENT_KEY_CHAR)
    {
        if (strlen(str) <= 24)
        {
            char temp[] = {evento.keyboard.unichar, '\0'};
            if (evento.keyboard.unichar == ' ')
            {
                strcat(str, temp);
            }
            else if (evento.keyboard.unichar >= '0' &&
                     evento.keyboard.unichar <= '9')
            {
                strcat(str, temp);
            }
            else if (evento.keyboard.unichar >= 'A' &&
                     evento.keyboard.unichar <= 'Z')
            {
                strcat(str, temp);
            }
            else if (evento.keyboard.unichar >= 'a' &&
                     evento.keyboard.unichar <= 'z')
            {
                strcat(str, temp);
            }
        }

        if (evento.keyboard.keycode == ALLEGRO_KEY_BACKSPACE && strlen(str) != 0)
        {
            str[strlen(str) - 1] = '\0';
        }
    }
}

